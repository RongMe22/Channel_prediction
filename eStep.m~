function X = eStep(parameter, csi)

alpha = parameter.alpha;
alpha = repmat(alpha,M,1);

phi = parameter.phi;
phi = repmat(phi,M,1);

tau = parameter.tau;
tau = repmat(tau,M,1);

C_M = repmat(transpose(0:M-1), 1, L);
C = exp(1j*2*pi/LAMBDA*D*C_M.*cos(phi));

S = sum(alpha.*C.*exp(-1j*2*pi*tau*FREQUENCY),2);
disp(csS);

global M L FREQUENCY

C = compute_C(parameter.phi);

S = repmat(parameter.alpha, M, 1) .* C .* ...
    exp(-1j*2*pi*FREQUENCY*repmat(parameter.tau, M, 1));
X = repmat(csi - sum(S, 2), 1,L) + S;
disp(repmat(csi - sum(S, 2), 1,L));
end

